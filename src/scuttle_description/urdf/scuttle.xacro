<?xml version="1.0" ?>
<robot name="scuttle" xmlns:xacro="http://www.ros.org/wiki/xacro">
    <xacro:include filename="$(find scuttle_description)/urdf/materials.xacro" />
    <xacro:include filename="$(find scuttle_description)/urdf/scuttle.trans" />
    <xacro:include filename="$(find scuttle_description)/urdf/scuttle.gazebo" />

    <!-- REMOVED ros2_control section - we're using DiffDrive plugin only -->

    <!-- Empty base link (required for simulation) -->
    <link name="base_link"/>

    <!-- Chassis connection -->
    <joint name="chassis_joint" type="fixed">
        <origin rpy="0 0 0" xyz="0.10539999389648432 0 0.084749938964843793"/>
        <parent link="base_link"/>
        <child link="chassis_link"/>
    </joint>

    <!-- Chassis link -->
    <link name="chassis_link">
        <inertial>
            <origin rpy="0 0 0" xyz="0.1 0 0.07392564365565002"/>
            <mass value="3.288323846275119"/>
            <inertia ixx="0.038876" ixy="1.3e-05" ixz="1e-06" iyy="0.047513" iyz="0.000486" izz="0.084083"/>
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://scuttle_description/meshes/scuttle_V3/visual/chassis_link.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="silver"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://scuttle_description/meshes/scuttle_V3/visual/chassis_link_collision.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <!-- Left wheel -->
    <joint name="l_wheel_joint" type="continuous">
        <origin rpy="0 0 1.570796" xyz="-0.10539999389648432 0.21235 -0.043"/>
        <parent link="chassis_link"/>
        <child link="l_wheel"/>
        <axis xyz="1.0 -0.0 0.0"/>
    </joint>

    <link name="l_wheel">
        <inertial>
            <origin rpy="0 0 0" xyz="-0.020325481156161784 -9.763736334427187e-09 4.516951403188418e-09"/>
            <mass value="0.5928646271111422"/>
            <inertia ixx="0.000334" ixy="0.0" ixz="-0.0" iyy="0.000332" iyz="-0.0" izz="0.000332"/>
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="-0.21235 -0.0 -0.04175"/>
            <geometry>
                <mesh filename="package://scuttle_description/meshes/scuttle_V3/visual/l_wheel_1.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="-0.21235 -0.0 -0.04175"/>
            <geometry>
                <mesh filename="package://scuttle_description/meshes/scuttle_V3/visual/l_wheel_collision_1.stl" scale="0.001 0.001 0.001"/>
            </geometry>
        </collision>
    </link>

    <!-- Right wheel -->
    <joint name="r_wheel_joint" type="continuous">
        <origin rpy="0 0 1.570796" xyz="-0.10539999389648432 -0.21235 -0.043"/>
        <parent link="chassis_link"/>
        <child link="r_wheel"/>
        <axis xyz="1.0 0.0 -0.0"/>
    </joint>

    <link name="r_wheel">
        <inertial>
            <origin rpy="0 0 0" xyz="0.020325563043213257 1.3954236146756306e-08 2.0353343177514915e-09"/>
            <mass value="0.592867092340216"/>
            <inertia ixx="0.000334" ixy="0.0" ixz="0.0" iyy="0.000332" iyz="-0.0" izz="0.000332"/>
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0.21215 0.0 -0.04175"/>
            <geometry>
                <mesh filename="package://scuttle_description/meshes/scuttle_V3/visual/r_wheel_1.stl" scale="0.001 0.001 0.001"/>
