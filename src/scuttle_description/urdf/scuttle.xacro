            <geometry>
                <mesh filename="package://scuttle_description/meshes/scuttle_V3/visual/l_caster_wheel_1.stl" scale="0.001 0.001 0.001"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
