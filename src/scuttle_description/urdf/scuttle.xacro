        <visual>
            <origin rpy="0 0 0" xyz="0.105071 0.230415 -0.025282"/>
            <geometry>
                <mesh filename="package://scuttle_description/meshes/scuttle_V3/visual/r_caster_wheel_1.stl" scale="0.001 0.001 0.001"/>
